{
  "name": "@ryanhefner/react-prismic",
  "version": "0.1.0",
  "description": "Easily compose Prismic requests and data into your React applications.",
  "repository": "ryanhefner/react-prismic",
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanhefner/react-prismic/issues"
  },
  "homepage": "https://github.com/ryanhefner/react-prismic#readme",
  "keywords": [
    "prismic",
    "prismic-javascript",
    "react",
    "react-provider",
    "react-context",
    "next-prismic",
    "query",
    "fetch",
    "ssr"
  ],
  "files": [
    "es",
    "src",
    "types",
    "umd",
    "getDataFromTree.js",
    "hoc-utils.js",
    "index.js",
    "PrismicClient.js",
    "PrismicContext.js",
    "PrismicProvider.js",
    "PrismicQuery.js",
    "RenderPromises.js",
    "withPrismic.js"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "types": "types",
  "scripts": {
    "clean": "rm -f index.js && rm -f PrismicClient.js && rm -f PrismicContext.js && rm -f PrismicProvider.js && rm -f getDataFromTree.js && rm -f hoc-utils.js && rm -f PrismicQuery.js && rm -f RenderPromises.js && rm -f withPrismic.js && rm -rf es && rm -rf umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js",
    "watch": "babel ./src -d . --ignore __mocks__,__tests__,**/*.test.js --watch",
    "prepare": "npm run build",
    "prepublishOnly": "node ./tools/build.js",
    "push-release": "git push origin master && git push --tags",
    "dtslint": "dtslint types",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=16.3",
    "react-dom": ">=16.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@types/react": "^16.9.2",
    "creature-cache": "^0.1.1",
    "flatted": "^2.0.1",
    "hoist-non-react-statics": "^3.3.0",
    "prismic-javascript": "^2.1.1",
    "prop-types": "^15.7.2",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "coveralls": "^3.0.6",
    "dtslint": "^0.9.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "gzip-size": "^5.1.1",
    "jest": "^24.8.0",
    "jest-enzyme": "^7.1.0",
    "jsdom": "^15.1.1",
    "pretty-bytes": "^5.3.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ]
  }
}
